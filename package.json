{
  "name": "sw-caching-recipe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "start:critical": "ngrok http 3004",
    "build": "npm-run-all --parallel build:*",
    "build:html": "node tasks/html.js",
    "build:scripts": "node tasks/scripts.js",
    "build:styles": "node tasks/styles.js",
    "build:service-worker": "ncp node_modules/sw-toolbox/sw-toolbox.js dist/sw-toolbox.js && ncp src/service-twerker.js dist/service-twerker.js",
    "prebuild": "rimraf dist/ && npm run copy",
    "postbuild": "npm-run-all --sequential critical revision",
    "compress": "node tasks/compress.js",
    "copy": "mkdirp dist/ && ncp src/ dist/",
    "critical": "node tasks/critical.js",
    "revision": "node tasks/revision-hash.js && node tasks/revision-replace.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:html": "chokidar 'src/**/*.html' --command 'npm run build:html'",
    "watch:scripts": "chokidar 'src/**/*.js' --command 'npm run build:scripts'",
    "watch:styles": "chokidar 'src/**/*.css' --command 'npm run build:styles'",
    "watch:service-worker": "chokidar 'src/service-twerker.js' --command 'npm run build:service-worker'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "accepts": "1.3.3",
    "chokidar-cli": "1.2.0",
    "cookie-parser": "1.4.3",
    "critical": "0.8.1",
    "event-stream": "3.3.4",
    "globby": "6.1.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-brotli": "1.2.0",
    "gulp-concat": "2.6.1",
    "gulp-cssnano": "2.1.2",
    "gulp-filter": "4.0.0",
    "gulp-gzip": "1.4.0",
    "gulp-htmlmin": "3.0.0",
    "gulp-rev": "7.1.2",
    "gulp-rev-css-url": "0.1.0",
    "gulp-rev-delete-original": "0.2.2",
    "gulp-rev-replace": "0.4.3",
    "gulp-uglify": "2.0.0",
    "gulp-wrap": "0.13.0",
    "mime": "1.3.4",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "ngrok": "2.2.5",
    "nodemon": "1.11.0",
    "npm-run-all": "3.1.2",
    "rimraf": "2.5.4",
    "shrink-ray": "0.1.3"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "express": "4.14.0",
    "nunjucks": "3.0.0",
    "sw-toolbox": "3.4.0"
  }
}
